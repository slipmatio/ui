{
  "name": "@slipmatio/ui",
  "version": "0.2.0",
  "type": "module",
  "description": "Common UI components for Slipmat.io projects",
  "author": {
    "name": "Ville Säävuori",
    "email": "ville@unessa.net"
  },
  "keywords": [
    "vue",
    "typescript",
    "slipmatio"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/slipmatio/ui"
  },
  "bugs": {
    "url": "https://github.com/slipmatio/ui/issues"
  },
  "homepage": "https://slipmat.io",
  "license": "MIT",
  "main": "./dist/ui.cjs",
  "module": "./dist/ui.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/ui.js",
      "require": "./dist/ui.cjs"
    },
    "./style.css": {
      "import": "./dist/style.css",
      "require": "./dist/style.css"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE.md"
  ],
  "sideEffects": false,
  "scripts": {
    "dev": "DEBUG=0 vite dev",
    "build": "DEBUG=0 vue-tsc --build --force && vite build",
    "test": "DEBUG=0 vitest",
    "test:e2e": "DEBUG=0 playwright test --ui",
    "type-check": "DEBUG=0 vue-tsc --build --force",
    "test:ci-e2e": "DEBUG=0 playwright test",
    "coverage": "DEBUG=0 vitest run --coverage"
  },
  "devDependencies": {
    "@playwright/test": "1.48.2",
    "@slipmatio/tailwind-config": "3.0.0",
    "@tsconfig/node22": "^22.0.0",
    "@types/node": "22.9.0",
    "@typescript-eslint/eslint-plugin": "8.14.0",
    "@vitejs/plugin-vue": "5.2.0",
    "@vitest/coverage-v8": "2.1.5",
    "@vitest/eslint-plugin": "1.1.10",
    "@vue/eslint-config-prettier": "10.1.0",
    "@vue/eslint-config-typescript": "14.1.3",
    "@vue/test-utils": "2.4.6",
    "@vue/tsconfig": "0.6.0",
    "autoprefixer": "10.4.20",
    "eslint-plugin-playwright": "2.0.1",
    "eslint-plugin-security": "3.0.1",
    "eslint-plugin-vue": "9.31.0",
    "happy-dom": "15.11.6",
    "postcss": "8.4.49",
    "tailwindcss": "3.4.15",
    "typescript": "5.6.3",
    "vite": "5.4.11",
    "vite-plugin-dts": "4.3.0",
    "vitest": "2.1.5",
    "vue": "3.5.13",
    "vue-tsc": "2.1.10"
  },
  "peerDependencies": {
    "@slipmatio/tailwind-config": "3",
    "vue": "3"
  },
  "engines": {
    "node": ">=22"
  }
}
